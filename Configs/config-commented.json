    /*sample command: /cd  1-15,2-25,3-13 */
    /* for 1-15 example 1 is the body index and 15 is the color number*/
    /*body index and colors are separated by quoma for easy execution*/
    "ColorDinoCMD": "/cd",
    /* example /cda rex 1-15,2-25,3-13 5000 */
    /* where 5000 is the radius range */
    /* just leave space /cda [notice space here replaces rex] 1-15,2-25,3-13 if you want all dinos within specified range */
    /* leave 5000 as blank if you want server maximum range */
    "ColorDinoAOECMD": "/cda",
    /* copy color via targeted dino*/
    "CopyColorDinoCMD": "/ccd",
    /* pasting color either target or AOE */
    "ApplyCopiedColorDinoCMD": "/accd",
    /* example /accda rex 5000 */
    /* just leave space /accda [notice space here replaces rex] 5000 if you want all dinos within specified range */
    "ApplyCopiedColorDinoAOECMD": "/accda"
  },
